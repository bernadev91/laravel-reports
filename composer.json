{
    "name": "bernadev/laravel-reports",
    "description": "Scaffolding to generate data reports in a unified way",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alejandro Bernabe",
            "email": "alex14xp@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "laravel/framework": ">=8.0.0"
    },
    "autoload": {
        "psr-4": {
            "Bernadev\\LaravelReports\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bernadev\\LaravelReports\\LaravelReportsServiceProvider"
            ]
        }
    }

}
